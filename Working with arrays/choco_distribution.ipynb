    "def minDiff(arr, m, l):\n",
    "    ans = 1000\n",
    "    arr.sort()\n",
    "    for i in range(l - (m - 1)):\n",
    "        if arr[i + m - 1] - arr[i] < ans:\n",
    "            ans = arr[i + m - 1] - arr[i]\n",
    "    return ans\n",
    "\n",
    "\n",
    "arr = [12, 4, 7, 9, 2, 23, 25]\n",
    "m = 4\n",
    "l = len(arr)\n",
    "print(minDiff(arr, m, l))"
   ]
  }
