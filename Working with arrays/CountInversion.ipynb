   "source": [
    "def inversion(arr):\n",
    "    ans = 0\n",
    "    for i in range(len(arr)):\n",
    "        for j in range(i + 1, len(arr)):\n",
    "            if arr[j] < arr[i]:\n",
    "                ans += 1\n",
    "    return ans\n",
    "\n",
    "\n",
    "array = [6, 3, 5, 2, 7]\n",
    "print(\"There are\", inversion(array), \"possible inversion\")"
   ]
  }
